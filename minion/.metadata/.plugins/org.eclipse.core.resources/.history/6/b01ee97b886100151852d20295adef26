package Classes;

import javax.swing.JFrame;

public class Janela extends JFrame {
	
	public Janela()
	{
		//alterando nome da janela
		super("Janela");
		//define o estilo de layout da janela
		setLayout(null);
		//definindo tamanho da janela
		setSize(500,500);
		//deixando janela visivel
		setVisible(true);
		
		
	}

}
